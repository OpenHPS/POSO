    <!-- http://purl.org/poso#AbsolutePosition -->


    <owl:Class rdf:about="http://purl.org/poso#AbsolutePosition">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#Position"/>
        <rdfs:comment xml:lang="en">An absolute position describes the position of an entity based on a fixed point in space. Usually this space is the Earth and the position is expressed in latitude and longitude.</rdfs:comment>
        <rdfs:label xml:lang="en">Absolute position</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#AngularMovement -->


    <owl:Class rdf:about="http://purl.org/poso#AngularMovement">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#Movement"/>
        <rdfs:comment xml:lang="en">Angular movement around a certain origin point.</rdfs:comment>
        <rdfs:label xml:lang="en">Angular movement</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#AngularVelocity -->


    <owl:Class rdf:about="http://purl.org/poso#AngularVelocity">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#Velocity"/>
        <rdfs:label xml:lang="en">Angular velocity</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#Angulation -->


    <owl:Class rdf:about="http://purl.org/poso#Angulation">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#Triangulation"/>
    </owl:Class>
    


    <!-- http://purl.org/poso#AuditoryLandmark -->


    <owl:Class rdf:about="http://purl.org/poso#AuditoryLandmark">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#Landmark"/>
        <rdfs:label xml:lang="en">Auditory landmark</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#CellIdentification -->


    <owl:Class rdf:about="http://purl.org/poso#CellIdentification">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#PositioningTechnique"/>
        <rdfs:comment xml:lang="en">Cell identification is a positioning procedure using the position of one landmark that is within the cell.</rdfs:comment>
        <rdfs:label xml:lang="en">Cell identification</rdfs:label>
        <skos:altLabel xml:lang="en">Cell Of Origin (COO)</skos:altLabel>
        <skos:example xml:lang="en">An example of CellIdentification is using the position of a Bluetooth beacon whenever this beacon has a high signal strength.</skos:example>
        <skos:example xml:lang="en">An example of CellIdentification is using the position of a QR-code when this code is scanned (and the user is presumably within a short distance).</skos:example>
    </owl:Class>
    


    <!-- http://purl.org/poso#DeadReckoning -->


    <owl:Class rdf:about="http://purl.org/poso#DeadReckoning">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#PositioningTechnique"/>
        <rdfs:comment xml:lang="en">Dead reckoning is the Procedure of calculating the current position of a moving FeatureOfInterest by using its previous position and Sensor Observation&apos;s indicating its heading and velocity.</rdfs:comment>
        <rdfs:label xml:lang="en">Dead reckoning</rdfs:label>
        <skos:altLabel xml:lang="en">Deduced reckoning</skos:altLabel>
    </owl:Class>
    


    <!-- http://purl.org/poso#Fingerprint -->


    <owl:Class rdf:about="http://purl.org/poso#Fingerprint">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/FeatureOfInterest"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Result"/>
        <rdfs:comment xml:lang="en">A fingerprint is a scene analysis</rdfs:comment>
        <rdfs:label xml:lang="en">Fingerprint</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#Fingerprinting -->


    <owl:Class rdf:about="http://purl.org/poso#Fingerprinting">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#PositioningTechnique"/>
        <rdfs:comment xml:lang="en">Fingerprinting is a positioning Procedure where sensor data is collected at a specific position and orientation. During the offline-stage of a positioning system, the significant data features are extracted and stored for that position. In the online-stage, the closest match(es) of the features are determined to predict the position.</rdfs:comment>
        <rdfs:label xml:lang="en">Fingerprinting</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#HybridPositioningSystem -->


    <owl:Class rdf:about="http://purl.org/poso#HybridPositioningSystem">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#PositioningSystem"/>
        <rdfs:comment xml:lang="en">A hybrid positioning system is a positioning system that makes use of multiple procedures to obtain a result.</rdfs:comment>
        <rdfs:label xml:lang="en">Hybrid positioning system</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#IndoorDeployment -->


    <owl:Class rdf:about="http://purl.org/poso#IndoorDeployment">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/Deployment"/>
        <rdfs:comment xml:lang="en">Describes the deployment of a System in an indoor environment.</rdfs:comment>
        <rdfs:label xml:lang="en">Indoor deployment</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#IndoorPositioningSystem -->


    <owl:Class rdf:about="http://purl.org/poso#IndoorPositioningSystem">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#PositioningSystem"/>
        <rdfs:comment xml:lang="en">A positioning system that is meant to perform indoor positioning of a feature of interest.</rdfs:comment>
        <rdfs:label xml:lang="en">Indoor positioning system</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#Landmark -->


    <owl:Class rdf:about="http://purl.org/poso#Landmark">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/FeatureOfInterest"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Result"/>
        <rdfs:comment xml:lang="en">A landmark is a feature with a known position, that can be used to identify the position of an object that has a relative position with this feature.</rdfs:comment>
        <rdfs:label xml:lang="en">Landmark</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#Lateration -->


    <owl:Class rdf:about="http://purl.org/poso#Lateration">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#Triangulation"/>
    </owl:Class>
    


    <!-- http://purl.org/poso#LinearMovement -->


    <owl:Class rdf:about="http://purl.org/poso#LinearMovement">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#Movement"/>
        <rdfs:comment xml:lang="en">Linear movement along a certain axis.</rdfs:comment>
        <rdfs:label xml:lang="en">Linear movement</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#LinearVelocity -->


    <owl:Class rdf:about="http://purl.org/poso#LinearVelocity">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#Velocity"/>
        <rdfs:label xml:lang="en">Linear velocity</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#LocationBasedService -->


    <owl:Class rdf:about="http://purl.org/poso#LocationBasedService">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/System"/>
        <rdfs:comment xml:lang="en">A location-based service (LBS) is a service that provides the location of a person or object. It provides this information without the required knowledge of the underlying technologies and algorithms.</rdfs:comment>
        <rdfs:label xml:lang="en">Location-based service</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#MagneticOdometry -->


    <owl:Class rdf:about="http://purl.org/poso#MagneticOdometry">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#Odometry"/>
        <rdfs:comment xml:lang="en">Magnetic field odometry is the procedure of detecting movement by analyizing how the magnetic field is changing from one Observation to another.</rdfs:comment>
        <rdfs:label xml:lang="en">Magnetic odometry</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#Movement -->


    <owl:Class rdf:about="http://purl.org/poso#Movement">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/Stimulus"/>
        <rdfs:comment xml:lang="en">Movement stimulus triggering an update of a position.</rdfs:comment>
        <rdfs:label xml:lang="en">Movement</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#Multilateration -->


    <owl:Class rdf:about="http://purl.org/poso#Multilateration">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#Lateration"/>
        <rdfs:comment xml:lang="en">Multilateration is the procedure of determining a position using relative distances to other known positions.</rdfs:comment>
        <rdfs:label xml:lang="en">Multilateration</rdfs:label>
        <skos:altLabel xml:lang="en">Hyperbolic positioning</skos:altLabel>
        <skos:altLabel xml:lang="en">Trilateration</skos:altLabel>
    </owl:Class>
    


    <!-- http://purl.org/poso#Odometry -->


    <owl:Class rdf:about="http://purl.org/poso#Odometry">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#PositioningTechnique"/>
        <rdfs:comment xml:lang="en">Odometry is the Procedure of calculating the current position of a moving FeatureOfInterest by using its previous position and Sensor Observation&apos;s indicating its heading and velocity.</rdfs:comment>
        <rdfs:label xml:lang="en">Odometry</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#Orientation -->


    <owl:Class rdf:about="http://purl.org/poso#Orientation">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Result"/>
        <rdfs:comment xml:lang="nl">De orientatie van een object in 2D of 3D.</rdfs:comment>
        <rdfs:comment xml:lang="en">The orientation of a feature of interest in 2D or 3D space.</rdfs:comment>
        <rdfs:label xml:lang="nl">Orientatie</rdfs:label>
        <rdfs:label xml:lang="en">Orientation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#OutdoorDeployment -->


    <owl:Class rdf:about="http://purl.org/poso#OutdoorDeployment">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/Deployment"/>
        <rdfs:comment xml:lang="en">Describes the deployment of a System in an outdoor environment.</rdfs:comment>
        <rdfs:label xml:lang="en">Outdoor deployment</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#Position -->


    <owl:Class rdf:about="http://purl.org/poso#Position">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Result"/>
        <rdfs:comment xml:lang="en">A position indicates where an entity is located.</rdfs:comment>
        <rdfs:label xml:lang="nl">Positie</rdfs:label>
        <rdfs:label xml:lang="en">Position</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#PositioningPlatform -->


    <owl:Class rdf:about="http://purl.org/poso#PositioningPlatform">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Platform"/>
        <rdfs:comment xml:lang="en">A positioning platform is a framework, architecture or platform used to develop and host a PositioningSystem.</rdfs:comment>
        <rdfs:label xml:lang="en">Positioning platform</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#PositioningSystem -->


    <owl:Class rdf:about="http://purl.org/poso#PositioningSystem">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/System"/>
        <rdfs:comment xml:lang="en">A positioning system is a system of instrumental and computational components for determining position.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">https://isotc211.geolexica.org/concepts/349/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Positioning system</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#PositioningTechnique -->


    <owl:Class rdf:about="http://purl.org/poso#PositioningTechnique">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Procedure"/>
        <rdfs:label xml:lang="en">Positioning technique</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#RFLandmark -->


    <owl:Class rdf:about="http://purl.org/poso#RFLandmark">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#Landmark"/>
        <rdfs:comment xml:lang="en">A radio frequency landmark is a transmitting landmark.</rdfs:comment>
        <rdfs:label xml:lang="en">RF landmark</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#RelativeAngle -->


    <owl:Class rdf:about="http://purl.org/poso#RelativeAngle">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#RelativePosition"/>
        <rdfs:label xml:lang="nl">Relatieve hoek</rdfs:label>
        <rdfs:label xml:lang="en">Relative angle</rdfs:label>
        <skos:example xml:lang="en">An example of relative angle is the Angle of Arrival.</skos:example>
    </owl:Class>
    


    <!-- http://purl.org/poso#RelativeDistance -->


    <owl:Class rdf:about="http://purl.org/poso#RelativeDistance">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#RelativePosition"/>
        <rdfs:label xml:lang="nl">Relatieve afstand</rdfs:label>
        <rdfs:label xml:lang="en">Relative distance</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#RelativePosition -->


    <owl:Class rdf:about="http://purl.org/poso#RelativePosition">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#Position"/>
        <rdfs:comment xml:lang="en">A relative position is a position of a feature of interest with respect to the positions of other objects that this position is relative to.</rdfs:comment>
        <rdfs:comment xml:lang="nl">Een relatieve positie is een positie van een punt, met respect tot de posities van andere objecten die relatief zijn.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">https://isotc211.geolexica.org/concepts/377/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="nl">Relatieve positie</rdfs:label>
        <rdfs:label xml:lang="en">Relative position</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#RelativeVelocity -->


    <owl:Class rdf:about="http://purl.org/poso#RelativeVelocity">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#RelativePosition"/>
        <rdfs:label xml:lang="nl">Relatieve snelheid</rdfs:label>
        <rdfs:label xml:lang="en">Relative velocity</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#SLAM -->


    <owl:Class rdf:about="http://purl.org/poso#SLAM">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#PositioningTechnique"/>
        <rdfs:comment xml:lang="en">Simultaneous localization and mapping (SLAM) is the computational problem of constructing or updating a map of an unknown environment while simultaneously keeping track of an agent&apos;s location within it.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">https://dbpedia.org/page/Simultaneous_localization_and_mapping</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Simultaneous localisation and mapping</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#SatellitePositioningSystem -->


    <owl:Class rdf:about="http://purl.org/poso#SatellitePositioningSystem">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso#PositioningSystem"/>
        <rdfs:label xml:lang="en">Satellite positioning system</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso#SensorFusion -->


    <owl:Class rdf:about="http://purl.org/poso#SensorFusion">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Procedure"/>
        <rdfs:comment xml:lang="en">Sensor fusion is a procedure where multiple sensor data is combined to obtain a more reliable or accurate result.</rdfs:comment>
        <rdfs:label xml:lang="en">Sensor fusion</rdfs:label>
    </owl:Class>
    


