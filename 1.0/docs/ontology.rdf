    <!-- http://purl.org/poso#hasAcceleration -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso#hasAcceleration">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
        <rdfs:label>has acceleration</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso#hasOrientation -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso#hasOrientation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
        <rdfs:comment xml:lang="en">Property that</rdfs:comment>
        <rdfs:label>has orientation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso#hasPosition -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso#hasPosition">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/FeatureOfInterest"/>
        <rdfs:range rdf:resource="http://purl.org/poso#Position"/>
        <rdfs:comment xml:lang="nl">Duid de absolute of relatieve positie van een object aan.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates the absolute or relative position of a feature of interest.</rdfs:comment>
        <rdfs:label xml:lang="en">has position</rdfs:label>
        <rdfs:label xml:lang="nl">heeft positie</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso#hasVelocity -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso#hasVelocity">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
        <rdfs:label xml:lang="en">has velocity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso#isRelativeTo -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso#isRelativeTo">
        <rdfs:domain rdf:resource="http://purl.org/poso#RelativePosition"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/sosa/FeatureOfInterest"/>
        <rdfs:label xml:lang="en">is relative to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso#usedSystem -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso#usedSystem">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/ssn/System"/>
        <rdfs:comment xml:lang="en">A relation to a re-usable system that computed the observation.</rdfs:comment>
        <rdfs:label xml:lang="en">used system</rdfs:label>
    </owl:ObjectProperty>
    


