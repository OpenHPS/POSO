    <!-- http://purl.org/poso/AbsolutePosition -->


    <owl:Class rdf:about="http://purl.org/poso/AbsolutePosition">
        <owl:equivalentClass rdf:resource="http://www.opengis.net/ont/geosparql#Geometry"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/poso/Position"/>
        <rdfs:comment xml:lang="en">An absolute position describes the position of an entity based on a fixed point in space. Usually this space is the Earth and the position is expressed in latitude and longitude.</rdfs:comment>
        <rdfs:label xml:lang="en">Absolute position</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso/Acceleration -->


    <owl:Class rdf:about="http://purl.org/poso/Acceleration">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Result"/>
        <qudt:hasQuantityKind rdf:resource="http://qudt.org/vocab/quantitykind/Acceleration"/>
        <rdfs:comment xml:lang="en">Acceleration is the (instantaneous) rate of change of velocity.</rdfs:comment>
        <rdfs:label xml:lang="en">Acceleration</rdfs:label>
        <rdfs:label xml:lang="nl">Versnelling</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso/AngularAcceleration -->


    <owl:Class rdf:about="http://purl.org/poso/AngularAcceleration">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso/Acceleration"/>
        <qudt:hasQuantityKind rdf:resource="http://qudt.org/vocab/quantitykind/AngularAcceleration"/>
        <rdfs:label xml:lang="en">Angular acceleration</rdfs:label>
        <rdfs:label xml:lang="nl">Draaiversnelling</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso/AngularMovement -->


    <owl:Class rdf:about="http://purl.org/poso/AngularMovement">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso/Movement"/>
        <rdfs:comment xml:lang="en">Angular movement around a certain origin point.</rdfs:comment>
        <rdfs:label xml:lang="en">Angular movement</rdfs:label>
        <rdfs:label xml:lang="nl">Draaibeweging</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso/AngularVelocity -->


    <owl:Class rdf:about="http://purl.org/poso/AngularVelocity">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso/Velocity"/>
        <qudt:hasQuantityKind rdf:resource="http://qudt.org/vocab/quantitykind/AngularVelocity"/>
        <rdfs:comment xml:lang="en">Angular velocity is the momentum around a point of origin.</rdfs:comment>
        <rdfs:label xml:lang="en">Angular velocity</rdfs:label>
        <rdfs:label xml:lang="nl">Draaisnelheid</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso/Angulation -->


    <owl:Class rdf:about="http://purl.org/poso/Angulation">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso/Triangulation"/>
        <rdfs:comment xml:lang="en">Angulation is a triangulation method to determine a position based on the relative angles to other objects.</rdfs:comment>
        <rdfs:label xml:lang="en">Angulation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso/AuditoryLandmark -->


    <owl:Class rdf:about="http://purl.org/poso/AuditoryLandmark">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso/Landmark"/>
        <rdfs:comment xml:lang="en">A spatial landmark that can be observed by sound.</rdfs:comment>
        <rdfs:label xml:lang="en">Auditory landmark</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso/AxisAngleOrientation -->


    <owl:Class rdf:about="http://purl.org/poso/AxisAngleOrientation">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso/Orientation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/poso/angle"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/poso/angle"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/poso/xAxisValue"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
