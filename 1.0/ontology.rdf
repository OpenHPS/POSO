        <rdfs:subClassOf rdf:resource="http://purl.org/poso/PositioningSystem"/>
        <rdfs:comment xml:lang="en">An inertial positioning system is a positioning system employing accelerometers, gyroscopes, and computer as integral components to determine coordinates of points or objects relative to an initial known reference point</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://isotc211.geolexica.org/concepts/240/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Inertial positioning system</rdfs:label>
    </owl:Class>
    


