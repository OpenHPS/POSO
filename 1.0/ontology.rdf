        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.thinkautonomous.ai/blog/?p=9-types-of-sensor-fusion-algorithms</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">High level sensor fusion</rdfs:label>
        <skos:altLabel xml:lang="en">Decision level sensor fusion</skos:altLabel>
    </owl:Class>
    


    <!-- http://purl.org/poso/IndoorDeployment -->


