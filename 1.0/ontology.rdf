        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Result"/>
        <rdfs:comment xml:lang="en">A fingerprint is a scene analysis at a particular absolute position.</rdfs:comment>
        <rdfs:label xml:lang="en">Fingerprint</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso/Fingerprinting -->


    <owl:Class rdf:about="http://purl.org/poso/Fingerprinting">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso/PositioningTechnique"/>
        <rdfs:comment xml:lang="en">Fingerprinting is a positioning Procedure where sensor data is collected at a specific position and orientation. During the offline-stage of a positioning system, the significant data features are extracted and stored for that position. In the online-stage, the closest match(es) of the features are determined to predict the position.</rdfs:comment>
        <rdfs:label xml:lang="en">Fingerprinting</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso/HighLevelFusion -->


    <owl:Class rdf:about="http://purl.org/poso/HighLevelFusion">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso/SensorFusion"/>
