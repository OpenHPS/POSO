    <!-- http://purl.org/poso/Fingerprint -->


    <owl:Class rdf:about="http://purl.org/poso/Fingerprint">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/poso/hasPosition"/>
