    <!-- http://purl.org/poso/angle -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/angle">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Result"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
        <rdfs:comment xml:lang="en">Quantitative anglue result value for axis-angle representation.</rdfs:comment>
        <rdfs:label xml:lang="en">angle</rdfs:label>
        <rdfs:label xml:lang="nl">hoek</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/hasAcceleration -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/hasAcceleration">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
        <owl:inverseOf rdf:resource="http://purl.org/poso/isAccelerationOf"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/FeatureOfInterest"/>
        <rdfs:range rdf:resource="http://purl.org/poso/Acceleration"/>
        <rdfs:comment xml:lang="en">Property that links a feature of interest to a unique acceleration property related to this feature.</rdfs:comment>
        <rdfs:label xml:lang="en">has acceleration</rdfs:label>
        <rdfs:label xml:lang="nl">heeft versnelling</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/hasAccuracy -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/hasAccuracy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment xml:lang="en">The accuracy of an entity.</rdfs:comment>
        <rdfs:label xml:lang="en">has accuracy</rdfs:label>
        <rdfs:label xml:lang="nl">heeft accuraatheid</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/hasCRS -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/hasCRS">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/poso/hasSRS"/>
        <rdfs:label xml:lang="en">has coordinate reference system</rdfs:label>
        <rdfs:label xml:lang="nl">heeft een co√∂rdinaten referentiesysteem</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/hasOrientation -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/hasOrientation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
        <owl:inverseOf rdf:resource="http://purl.org/poso/isOrientationOf"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/FeatureOfInterest"/>
        <rdfs:range rdf:resource="http://purl.org/poso/Orientation"/>
        <rdfs:comment xml:lang="nl">Duid de orientatie aan van een object aan.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates the orientation of a feature of interest.</rdfs:comment>
        <rdfs:label xml:lang="en">has orientation</rdfs:label>
        <rdfs:label xml:lang="nl">heeft orientatie</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/hasPosition -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/hasPosition">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
        <owl:inverseOf rdf:resource="http://purl.org/poso/isPositionOf"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/FeatureOfInterest"/>
        <rdfs:range rdf:resource="http://purl.org/poso/Position"/>
        <rdfs:comment xml:lang="nl">Duid de absolute of relatieve positie van een object aan.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates the absolute or relative position of a feature of interest.</rdfs:comment>
        <rdfs:label xml:lang="en">has position</rdfs:label>
        <rdfs:label xml:lang="nl">heeft positie</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/hasRelativePosition -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/hasRelativePosition">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/poso/hasPosition"/>
        <owl:inverseOf rdf:resource="http://purl.org/poso/isRelativeTo"/>
        <rdfs:range rdf:resource="http://purl.org/poso/RelativePosition"/>
        <rdfs:label xml:lang="en">has relative position</rdfs:label>
        <rdfs:label xml:lang="nl">heeft relatieve positie</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/hasSRS -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/hasSRS">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <qudt:example rdf:resource="http://www.opengis.net/def/crs/OGC/1.3/CRS84"/>
        <rdfs:comment xml:lang="en">Identifies an entity that has a spatial reference system in order to interpret the result.</rdfs:comment>
        <rdfs:label xml:lang="en">has spatial reference system</rdfs:label>
        <rdfs:label xml:lang="nl">heeft een ruimtelijk referentiepunt</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/hasVelocity -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/hasVelocity">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
        <owl:inverseOf rdf:resource="http://purl.org/poso/isVelocityOf"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/FeatureOfInterest"/>
        <rdfs:range rdf:resource="http://purl.org/poso/Velocity"/>
        <rdfs:comment xml:lang="nl">Duid de snelheid van een object aan.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates the velocity of a feature of interest.</rdfs:comment>
        <rdfs:label xml:lang="en">has velocity</rdfs:label>
        <rdfs:label xml:lang="nl">heeft snelheid</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/inDeployment -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/inDeployment">
        <rdfs:domain rdf:resource="http://purl.org/poso/AbsolutePosition"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/ssn/Deployment"/>
        <vann:preferredNamespaceUri xml:lang="en">Identifies an absolute position within a deployment</vann:preferredNamespaceUri>
        <rdfs:label xml:lang="en">in deployment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/isAccelerationOf -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/isAccelerationOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/isPropertyOf"/>
        <rdfs:label xml:lang="en">is acceleration of</rdfs:label>
        <rdfs:label xml:lang="nl">is versnelling van</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/isOrientationOf -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/isOrientationOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/isPropertyOf"/>
        <rdfs:label xml:lang="nl">is orientatie van</rdfs:label>
        <rdfs:label xml:lang="en">is orientation of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/isPositionOf -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/isPositionOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/isPropertyOf"/>
        <rdfs:label xml:lang="nl">is positie van</rdfs:label>
        <rdfs:label xml:lang="en">is position of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/isRelativeTo -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/isRelativeTo">
        <rdfs:domain rdf:resource="http://purl.org/poso/RelativePosition"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/sosa/FeatureOfInterest"/>
        <rdfs:comment xml:lang="en">Indicates a relative position to be relative to another feature of interest.</rdfs:comment>
        <rdfs:label xml:lang="nl">is relatief met</rdfs:label>
        <rdfs:label xml:lang="en">is relative to</rdfs:label>
        <skos:example xml:lang="en">A person can have a relative distance to another feature of interest. When a feature is moving (i.e. when the geometry changes based on time) the distance remains relative to the feature and not the geometry at a specific point in time.</skos:example>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/isVelocityOf -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/isVelocityOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/isPropertyOf"/>
        <rdfs:label xml:lang="nl">is de snelheid van</rdfs:label>
        <rdfs:label xml:lang="en">is velocity of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/madeBySystem -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/madeBySystem">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/ssn/System"/>
        <rdfs:comment xml:lang="en">A relation to a re-usable system that computed the observation.</rdfs:comment>
        <rdfs:label xml:lang="nl">gemaakt door systeem</rdfs:label>
        <rdfs:label xml:lang="en">made by system</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/observesType -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/observesType">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/forProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/ssn/System"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
        <rdfs:comment xml:lang="en">The relation between a system and an observable property class to identify that a system observes a specific type of property.</rdfs:comment>
        <rdfs:label xml:lang="nl">observeert type</rdfs:label>
        <rdfs:label xml:lang="en">observes type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/pitch -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/pitch">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/poso/EulerOrientation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
        <rdfs:comment xml:lang="en">Pitch is the rotation around the x-axis with respect to the object the yaw applies to.</rdfs:comment>
        <rdfs:label xml:lang="en">pitch</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/roll -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/roll">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/poso/EulerOrientation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
        <rdfs:comment xml:lang="en">Roll is the rotation around the y-axis with respect to the object the roll applies to.</rdfs:comment>
        <rdfs:label xml:lang="en">roll</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/scalar -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/scalar">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Result"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
        <rdfs:label xml:lang="en">scalar</rdfs:label>
        <skos:altLabel>w</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/xAxisValue -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/xAxisValue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Result"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
        <rdfs:comment xml:lang="en">Quantitative result value along the X-axis of a spatial sensor or result.</rdfs:comment>
        <rdfs:label xml:lang="en">x-axis value</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/yAxisValue -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/yAxisValue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Result"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
        <rdfs:comment xml:lang="en">Quantitative result value along the Y-axis of a spatial sensor or result.</rdfs:comment>
        <rdfs:label xml:lang="en">y-axis value</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/yaw -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/yaw">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/poso/EulerOrientation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
        <rdfs:comment xml:lang="en">Yaw is the rotation around the z-axis with respect to the object the yaw applies to.</rdfs:comment>
        <rdfs:label xml:lang="en">yaw</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/poso/zAxisValue -->


    <owl:ObjectProperty rdf:about="http://purl.org/poso/zAxisValue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Result"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
        <rdfs:comment xml:lang="en">Quantitative result value along the Z-axis of a spatial sensor.</rdfs:comment>
        <rdfs:label xml:lang="en">z-axis value</rdfs:label>
    </owl:ObjectProperty>
    


