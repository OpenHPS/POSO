#################################################################
#    Object Properties
#################################################################

###  http://purl.org/poso#hasAcceleration
:hasAcceleration rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://www.w3.org/ns/ssn/hasProperty> ;
                 rdfs:domain <http://www.w3.org/ns/sosa/FeatureOfInterest> ;
                 rdfs:label "has acceleration" .


###  http://purl.org/poso#hasAccuracy
:hasAccuracy rdf:type owl:ObjectProperty ;
             owl:equivalentProperty <http://www.opengis.net/ont/geosparql#hasSpatialAccuracy> ;
             rdfs:subPropertyOf <http://www.w3.org/ns/ssn/systems/qualityOfObservation> ;
             rdf:type owl:FunctionalProperty ;
             rdfs:comment "The accuracy of an observation or observation result."@en ;
             rdfs:label "has accuracy"@en .


###  http://purl.org/poso#hasOrientation
:hasOrientation rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://www.w3.org/ns/ssn/hasProperty> ;
                rdfs:domain <http://www.w3.org/ns/sosa/FeatureOfInterest> ;
                rdfs:range :Orientation ;
                rdfs:comment "Property that"@en ;
                rdfs:label "has orientation" .


###  http://purl.org/poso#hasPosition
:hasPosition rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://www.w3.org/ns/ssn/hasProperty> ;
             rdfs:domain <http://www.w3.org/ns/sosa/FeatureOfInterest> ;
             rdfs:range :Position ;
             rdfs:comment "Duid de absolute of relatieve positie van een object aan."@nl ,
                          "Indicates the absolute or relative position of a feature of interest."@en ;
             rdfs:label "has position"@en ,
                        "heeft positie"@nl .


###  http://purl.org/poso#hasVelocity
:hasVelocity rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://www.w3.org/ns/ssn/hasProperty> ;
             rdfs:domain <http://www.w3.org/ns/sosa/FeatureOfInterest> ;
             rdfs:range :Velocity ;
             rdfs:label "has velocity"@en .


###  http://purl.org/poso#isRelativeTo
:isRelativeTo rdf:type owl:ObjectProperty ;
              rdfs:domain :RelativePosition ;
              rdfs:range <http://www.w3.org/ns/sosa/FeatureOfInterest> ;
              rdfs:comment "Indicates a relative position to be relative to another feature."@en ;
              rdfs:label "is relative to"@en .


###  http://purl.org/poso#usedSystem
:usedSystem rdf:type owl:ObjectProperty ;
            rdfs:domain <http://www.w3.org/ns/sosa/Observation> ;
            rdfs:range <http://www.w3.org/ns/ssn/System> ;
            rdfs:comment "A relation to a re-usable system that computed the observation."@en ;
            rdfs:label "used system"@en .


###  http://purl.org/poso#xAxisValue
:xAxisValue rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain <http://www.w3.org/ns/sosa/Result> ;
            rdfs:label "x-axis value"@en .


###  http://purl.org/poso#yAxisValue
:yAxisValue rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain <http://www.w3.org/ns/sosa/Value> ;
            rdfs:label "y-axis value"@en .


###  http://purl.org/poso#zAxisValue
:zAxisValue rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain <http://www.w3.org/ns/sosa/Value> ;
            rdfs:label "z-axis value"@en .


###  http://www.opengis.net/ont/geosparql#hasSpatialAccuracy

#################################################################
#    Classes
#################################################################

###  http://purl.org/poso#AbsolutePosition
:AbsolutePosition rdf:type owl:Class ;
                  owl:equivalentClass <http://www.opengis.net/ont/geosparql#Geometry> ;
                  rdfs:subClassOf :Position ;
                  rdfs:comment "An absolute position describes the position of an entity based on a fixed point in space. Usually this space is the Earth and the position is expressed in latitude and longitude."@en ;
                  rdfs:label "Absolute position"@en .


###  http://purl.org/poso#AbsolutePositionProperty
:AbsolutePositionProperty rdf:type owl:Class ;
                          rdfs:comment "An observable absolute position property of a FeatureOfInterest."@en ;
                          rdfs:label "Absolute Position Property"@en .


###  http://purl.org/poso#Acceleration
:Acceleration rdf:type owl:Class ;
              rdfs:subClassOf <http://www.w3.org/ns/sosa/ObservableProperty> ,
                              <http://www.w3.org/ns/sosa/Result> ;
              <http://qudt.org/schema/qudt/hasQuantityKind> <http://qudt.org/vocab/quantitykind/Acceleration> ;
              rdfs:comment "Acceleration cceleration is the (instantaneous) rate of change of velocity."@en ;
              rdfs:label "Acceleration"@en .


###  http://purl.org/poso#AngularAcceleration
:AngularAcceleration rdf:type owl:Class ;
                     rdfs:subClassOf :Acceleration ;
                     <http://qudt.org/schema/qudt/hasQuantityKind> <http://qudt.org/vocab/quantitykind/AngularAcceleration> ;
                     rdfs:label "Angular acceleration"@en .


###  http://purl.org/poso#AngularAccelerationProperty
:AngularAccelerationProperty rdf:type owl:Class .


###  http://purl.org/poso#AngularMovement
:AngularMovement rdf:type owl:Class ;
                 rdfs:subClassOf :Movement ;
                 rdfs:comment "Angular movement around a certain origin point."@en ;
                 rdfs:label "Angular movement"@en .


###  http://purl.org/poso#AngularVelocity
:AngularVelocity rdf:type owl:Class ;
                 rdfs:subClassOf :Velocity ;
                 <http://qudt.org/schema/qudt/hasQuantityKind> <http://qudt.org/vocab/quantitykind/AngularVelocity> ;
                 rdfs:label "Angular velocity"@en .


###  http://purl.org/poso#AngularVelocityProperty
:AngularVelocityProperty rdf:type owl:Class .


###  http://purl.org/poso#Angulation
:Angulation rdf:type owl:Class ;
            rdfs:subClassOf :Triangulation .


###  http://purl.org/poso#AuditoryLandmark
:AuditoryLandmark rdf:type owl:Class ;
                  rdfs:subClassOf :Landmark ;
                  rdfs:label "Auditory landmark"@en .


###  http://purl.org/poso#AxisAngleOrientation
:AxisAngleOrientation rdf:type owl:Class ;
                      rdfs:subClassOf :Orientation .


###  http://purl.org/poso#CellIdentification
:CellIdentification rdf:type owl:Class ;
                    rdfs:subClassOf :PositioningTechnique ;
                    rdfs:comment "Cell identification is a positioning procedure using the position of one landmark that is within the cell."@en ;
                    rdfs:label "Cell identification"@en ;
                    <http://www.w3.org/2004/02/skos/core#altLabel> "Cell Of Origin (COO)"@en ;
                    <http://www.w3.org/2004/02/skos/core#example> "An example of CellIdentification is using the position of a Bluetooth beacon whenever this beacon has a high signal strength."@en ,
                                                                  "An example of CellIdentification is using the position of a QR-code when this code is scanned (and the user is presumably within a short distance)."@en .


###  http://purl.org/poso#DeadReckoning
:DeadReckoning rdf:type owl:Class ;
               rdfs:subClassOf :PositioningTechnique ;
               rdfs:comment "Dead reckoning is the Procedure of calculating the current position of a moving FeatureOfInterest by using its previous position and Sensor Observation's indicating its heading and velocity."@en ;
               rdfs:label "Dead reckoning"@en ;
               <http://www.w3.org/2004/02/skos/core#altLabel> "Deduced reckoning"@en .


###  http://purl.org/poso#EulerOrientation
:EulerOrientation rdf:type owl:Class ;
                  rdfs:subClassOf :Orientation .


###  http://purl.org/poso#Fingerprint
:Fingerprint rdf:type owl:Class ;
             rdfs:subClassOf <http://www.w3.org/ns/sosa/FeatureOfInterest> ,
                             <http://www.w3.org/ns/sosa/ObservableProperty> ,
                             <http://www.w3.org/ns/sosa/Result> ;
             rdfs:comment "A fingerprint is a scene analysis"@en ;
             rdfs:label "Fingerprint"@en .


###  http://purl.org/poso#Fingerprinting
:Fingerprinting rdf:type owl:Class ;
                rdfs:subClassOf :PositioningTechnique ;
                rdfs:comment "Fingerprinting is a positioning Procedure where sensor data is collected at a specific position and orientation. During the offline-stage of a positioning system, the significant data features are extracted and stored for that position. In the online-stage, the closest match(es) of the features are determined to predict the position."@en ;
                rdfs:label "Fingerprinting"@en .


###  http://purl.org/poso#HybridPositioningSystem
:HybridPositioningSystem rdf:type owl:Class ;
                         rdfs:subClassOf :PositioningSystem ;
                         rdfs:comment "An integrated positioning system is a positioning system that incorporates multiple positioning technologies."@en ;
                         rdfs:isDefinedBy <https://isotc211.geolexica.org/concepts/250/> ;
                         rdfs:label "Integrated positioning system"@en ;
                         <http://www.w3.org/2004/02/skos/core#altLabel> "Hybrid positioning system"@en .


###  http://purl.org/poso#IndoorDeployment
:IndoorDeployment rdf:type owl:Class ;
                  rdfs:subClassOf <http://www.w3.org/ns/ssn/Deployment> ;
                  rdfs:comment "Describes the deployment of a System in an indoor environment."@en ;
                  rdfs:label "Indoor deployment"@en .


###  http://purl.org/poso#IndoorPositioningSystem
:IndoorPositioningSystem rdf:type owl:Class ;
                         rdfs:subClassOf :PositioningSystem ;
                         rdfs:comment "A positioning system that is meant to perform indoor positioning of a feature of interest."@en ;
                         rdfs:label "Indoor positioning system"@en .


###  http://purl.org/poso#InertialPositioningSystem
:InertialPositioningSystem rdf:type owl:Class ;
                           rdfs:subClassOf :PositioningSystem ;
                           rdfs:comment "An inertial positioning system is a positioning system employing accelerometers, gyroscopes, and computer as integral components to determine coordinates of points or objects relative to an initial known reference point"@en ;
                           rdfs:isDefinedBy <https://isotc211.geolexica.org/concepts/240/> ;
                           rdfs:label "Inertial positioning system"@en .


###  http://purl.org/poso#Landmark
:Landmark rdf:type owl:Class ;
          rdfs:subClassOf <http://www.w3.org/ns/sosa/FeatureOfInterest> ,
                          <http://www.w3.org/ns/sosa/ObservableProperty> ,
                          <http://www.w3.org/ns/sosa/Result> ;
          rdfs:comment "A landmark is a feature with a known position, that can be used to identify the position of an object that has a relative position with this feature."@en ;
          rdfs:label "Landmark"@en .


###  http://purl.org/poso#Lateration
:Lateration rdf:type owl:Class ;
            rdfs:subClassOf :Triangulation .


###  http://purl.org/poso#LinearAcceleration
:LinearAcceleration rdf:type owl:Class ;
                    rdfs:subClassOf :Acceleration ;
                    <http://qudt.org/schema/qudt/hasQuantityKind> <http://qudt.org/vocab/quantitykind/LinearAcceleration> ;
                    rdfs:label "Linear acceleration"@en .


###  http://purl.org/poso#LinearAccelerationProperty
:LinearAccelerationProperty rdf:type owl:Class .


###  http://purl.org/poso#LinearMovement
:LinearMovement rdf:type owl:Class ;
                rdfs:subClassOf :Movement ;
                rdfs:comment "Linear movement along a certain axis."@en ;
                rdfs:label "Linear movement"@en .


###  http://purl.org/poso#LinearVelocity
:LinearVelocity rdf:type owl:Class ;
                rdfs:subClassOf :Velocity ;
                <http://qudt.org/schema/qudt/hasQuantityKind> <http://qudt.org/vocab/quantitykind/LinearVelocity> ;
                rdfs:label "Linear velocity"@en .


###  http://purl.org/poso#LinearVelocityProperty
:LinearVelocityProperty rdf:type owl:Class .


###  http://purl.org/poso#LocationBasedService
:LocationBasedService rdf:type owl:Class ;
                      rdfs:subClassOf <http://www.w3.org/ns/ssn/System> ;
                      rdfs:comment "A location based service (LBS) is a service that provides the location of a person or object. It provides this information without the required knowledge of the underlying technologies and algorithms."@en ;
                      rdfs:isDefinedBy <https://isotc211.geolexica.org/concepts/278/> ;
                      rdfs:label "Location based service"@en .


###  http://purl.org/poso#MagneticOdometry
:MagneticOdometry rdf:type owl:Class ;
                  rdfs:subClassOf :Odometry ;
                  rdfs:comment "Magnetic field odometry is the procedure of detecting movement by analyizing how the magnetic field is changing from one Observation to another."@en ;
                  rdfs:label "Magnetic odometry"@en .


###  http://purl.org/poso#Movement
:Movement rdf:type owl:Class ;
          rdfs:subClassOf <http://www.w3.org/ns/ssn/Stimulus> ;
          rdfs:comment "Movement stimulus triggering an update of a position."@en ;
          rdfs:label "Movement"@en .


###  http://purl.org/poso#Multilateration
:Multilateration rdf:type owl:Class ;
                 rdfs:subClassOf :Lateration ;
                 rdfs:comment "Multilateration is the procedure of determining a position using relative distances to other known positions."@en ;
                 rdfs:label "Multilateration"@en ;
                 <http://www.w3.org/2004/02/skos/core#altLabel> "Hyperbolic positioning"@en ,
                                                                "Trilateration"@en .


###  http://purl.org/poso#Odometry
:Odometry rdf:type owl:Class ;
          rdfs:subClassOf :PositioningTechnique ;
          rdfs:comment "Odometry is the Procedure of calculating the current position of a moving FeatureOfInterest by using its previous position and Sensor Observation's indicating its heading and velocity."@en ;
          rdfs:label "Odometry"@en .


###  http://purl.org/poso#OpticalPositioningSystem
:OpticalPositioningSystem rdf:type owl:Class ;
                          rdfs:subClassOf :PositioningSystem ;
                          rdfs:comment "An optical positioning system is a positioning system that determines the position of an object by means of the properties of light."@en ;
                          rdfs:isDefinedBy <https://isotc211.geolexica.org/concepts/320/> ;
                          rdfs:label "Optical positioning system"@en .


###  http://purl.org/poso#Orientation
:Orientation rdf:type owl:Class ;
             rdfs:subClassOf <http://www.w3.org/ns/sosa/ObservableProperty> ,
                             <http://www.w3.org/ns/sosa/Result> ;
             rdfs:comment "De orientatie van een object in 2D of 3D."@nl ,
                          "The orientation of a feature of interest in 2D or 3D space."@en ;
             rdfs:label "Orientatie"@nl ,
                        "Orientation"@en .


###  http://purl.org/poso#OutdoorDeployment
:OutdoorDeployment rdf:type owl:Class ;
                   rdfs:subClassOf <http://www.w3.org/ns/ssn/Deployment> ;
                   rdfs:comment "Describes the deployment of a System in an outdoor environment."@en ;
                   rdfs:label "Outdoor deployment"@en .


###  http://purl.org/poso#Position
:Position rdf:type owl:Class ;
          rdfs:subClassOf <http://www.w3.org/ns/sosa/ObservableProperty> ,
                          <http://www.w3.org/ns/sosa/Result> ;
          rdfs:comment "A position indicates where an entity is located."@en ;
          rdfs:label "Positie"@nl ,
                     "Position"@en .


###  http://purl.org/poso#PositioningPlatform
:PositioningPlatform rdf:type owl:Class ;
                     rdfs:subClassOf <http://www.w3.org/ns/sosa/Platform> ;
                     rdfs:comment "A positioning platform is a framework, architecture or platform used to develop and host a PositioningSystem."@en ;
                     rdfs:label "Positioning platform"@en .


###  http://purl.org/poso#PositioningSystem
:PositioningSystem rdf:type owl:Class ;
                   rdfs:subClassOf <http://www.w3.org/ns/ssn/System> ;
                   rdfs:comment "A positioning system is a system of instrumental and computational components for determining position."@en ;
                   rdfs:isDefinedBy "https://isotc211.geolexica.org/concepts/349/"@en ;
                   rdfs:label "Positioning system"@en .


###  http://purl.org/poso#PositioningTechnique
:PositioningTechnique rdf:type owl:Class ;
                      rdfs:subClassOf <http://www.w3.org/ns/sosa/Procedure> ;
                      rdfs:label "Positioning technique"@en .


###  http://purl.org/poso#QuaternionOrientation
:QuaternionOrientation rdf:type owl:Class ;
                       rdfs:subClassOf :Orientation .


###  http://purl.org/poso#RFLandmark
:RFLandmark rdf:type owl:Class ;
            rdfs:subClassOf :Landmark ;
            rdfs:comment "A radio frequency landmark is a transmitting landmark."@en ;
            rdfs:label "RF landmark"@en .


###  http://purl.org/poso#RadioPropagation
:RadioPropagation rdf:type owl:Class ;
                  rdfs:subClassOf <http://www.w3.org/ns/sosa/Procedure> ;
                  rdfs:comment "The radio propagation formulas cover the computation of the radio waves through a medium (e.g. air)."@en ;
                  rdfs:label "Radio propagation"@en .


###  http://purl.org/poso#RelativeAngle
:RelativeAngle rdf:type owl:Class ;
               rdfs:subClassOf :RelativePosition ;
               rdfs:label "Relatieve hoek"@nl ,
                          "Relative angle"@en ;
               <http://www.w3.org/2004/02/skos/core#example> "An example of relative angle is the Angle of Arrival."@en .


###  http://purl.org/poso#RelativeDistance
:RelativeDistance rdf:type owl:Class ;
                  rdfs:subClassOf :RelativePosition ;
                  rdfs:label "Relatieve afstand"@nl ,
                             "Relative distance"@en .


###  http://purl.org/poso#RelativePosition
:RelativePosition rdf:type owl:Class ;
                  rdfs:subClassOf :Position ;
                  rdfs:comment "A relative position is a position of a feature of interest with respect to the positions of other objects that this position is relative to."@en ,
                               "Een relatieve positie is een positie van een punt, met respect tot de posities van andere objecten die relatief zijn."@nl ;
                  rdfs:isDefinedBy "https://isotc211.geolexica.org/concepts/377/"@en ;
                  rdfs:label "Relatieve positie"@nl ,
                             "Relative position"@en .


###  http://purl.org/poso#RelativePositionProperty
:RelativePositionProperty rdf:type owl:Class ;
                          rdfs:comment "An observable relative position property of a FeatureOfInterest."@en ;
                          rdfs:label "Relative Position Property"@en .


###  http://purl.org/poso#RelativeVelocity
:RelativeVelocity rdf:type owl:Class ;
                  rdfs:subClassOf :RelativePosition ;
                  rdfs:label "Relatieve snelheid"@nl ,
                             "Relative velocity"@en .


###  http://purl.org/poso#SLAM
:SLAM rdf:type owl:Class ;
      rdfs:subClassOf :PositioningTechnique ;
      rdfs:comment "Simultaneous localization and mapping (SLAM) is the computational problem of constructing or updating a map of an unknown environment while simultaneously keeping track of an agent's location within it."@en ;
      rdfs:isDefinedBy "https://dbpedia.org/page/Simultaneous_localization_and_mapping"@en ;
      rdfs:label "Simultaneous localisation and mapping"@en .


###  http://purl.org/poso#SatellitePositioningSystem
:SatellitePositioningSystem rdf:type owl:Class ;
                            rdfs:subClassOf :PositioningSystem ;
                            rdfs:isDefinedBy <https://isotc211.geolexica.org/concepts/392/> ;
                            rdfs:label "Satellite positioning system"@en .


###  http://purl.org/poso#SensorFusion
:SensorFusion rdf:type owl:Class ;
              rdfs:subClassOf <http://www.w3.org/ns/sosa/Procedure> ;
              rdfs:comment "Sensor fusion is a procedure where multiple sensor data is combined to obtain a more reliable or accurate result."@en ;
              rdfs:label "Sensor fusion"@en .


###  http://purl.org/poso#TrackedFeature
:TrackedFeature rdf:type owl:Class ;
                rdfs:subClassOf <http://www.w3.org/ns/sosa/FeatureOfInterest> ;
                rdfs:label "Tracked feature"@en .


###  http://purl.org/poso#Triangulation
:Triangulation rdf:type owl:Class ;
               rdfs:subClassOf :PositioningTechnique ;
               rdfs:comment "Triangulation is the procedure of determining a position using relative angles to a feature of interest with a known position."@en ;
               rdfs:label "Triangulation"@en .


###  http://purl.org/poso#VSLAM
:VSLAM rdf:type owl:Class ;
       rdfs:subClassOf :SLAM ;
       rdfs:comment "Visual simultaneous localisation and mapping (VSLAM) is a positioning techniques that uses visual imagery to map an environment. Positioning works by detecting features of interest in image frames and comparing how these features move from one frame to another."@en ;
       rdfs:label "Visual simultaneous localisation and mapping"@en .


###  http://purl.org/poso#Velocity
:Velocity rdf:type owl:Class ;
          rdfs:subClassOf <http://www.w3.org/ns/sosa/ObservableProperty> ,
                          <http://www.w3.org/ns/sosa/Result> ;
          <http://qudt.org/schema/qudt/hasQuantityKind> <http://qudt.org/vocab/quantitykind/Velocity> ;
          rdfs:comment "Linear or angular velocity of a feature."@en ;
          rdfs:label "Velocity"@en .


###  http://purl.org/poso#VisualLandmark
:VisualLandmark rdf:type owl:Class ;
                rdfs:subClassOf :Landmark ;
                rdfs:comment "A visual landmark is a feature of interest that visually present in the space."@en ;
                rdfs:label "Visual landmark"@en .


###  http://purl.org/poso#VisualOdometry
:VisualOdometry rdf:type owl:Class ;
                rdfs:subClassOf :Odometry ;
