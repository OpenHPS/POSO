    <!-- http://purl.org/poso/common/Anyplace -->


    <owl:NamedIndividual rdf:about="http://purl.org/poso/common/Anyplace">
        <rdf:type rdf:resource="http://purl.org/poso/PositioningPlatform"/>
        <rdfs:comment xml:lang="en">Anyplace is a first-of-a-kind indoor information service offering GPS-less localization, navigation and search inside buildings using ordinary smartphones.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://anyplace.cs.ucy.ac.cy/"/>
        <rdfs:label xml:lang="en">Anyplace</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/poso/common/BDS -->


    <owl:NamedIndividual rdf:about="http://purl.org/poso/common/BDS">
        <rdf:type rdf:resource="http://purl.org/poso/SatellitePositioningSystem"/>
        <rdfs:comment xml:lang="en">BeiDou, or BDS, is a global GNSS owned and operated by the People&apos;s Republic of China. BDS was formally commissioned in 2020. The operational system consists of 35 satellites. BDS was previously called Compass.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.gps.gov/systems/gnss/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">BeiDou Navigation Satellite System</rdfs:label>
        <ssn:hasDeployment rdf:resource="http://purl.org/poso/common/EarthDeployment"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/poso/common/CSIFingerprinting -->


    <owl:NamedIndividual rdf:about="http://purl.org/poso/common/CSIFingerprinting">
        <rdf:type rdf:resource="http://purl.org/poso/Fingerprinting"/>
        <rdfs:label xml:lang="en">Channel State Information fingerprinting</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/poso/common/EarthDeployment -->


    <owl:NamedIndividual rdf:about="http://purl.org/poso/common/EarthDeployment">
        <rdf:type rdf:resource="http://purl.org/poso/OutdoorDeployment"/>
        <rdfs:comment xml:lang="en">A generic deployment for any positioning system deployment on Earth.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://dbpedia.org/resouce/Earth"/>
        <rdfs:label xml:lang="en">Earth deployment</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/poso/common/FootMountedPDR -->


    <owl:NamedIndividual rdf:about="http://purl.org/poso/common/FootMountedPDR">
        <rdf:type rdf:resource="http://purl.org/poso/PDR"/>
        <rdfs:comment xml:lang="en">Foot-mounted pedestrian dead reckoning uses a sensor on the foot of a pedestrian to detect steps.</rdfs:comment>
        <rdfs:label xml:lang="en">Foot-mounted pedestrian dead reckoning</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/poso/common/GLONASS -->


    <owl:NamedIndividual rdf:about="http://purl.org/poso/common/GLONASS">
        <rdf:type rdf:resource="http://purl.org/poso/SatellitePositioningSystem"/>
        <rdfs:comment xml:lang="en">GLONASS (Globalnaya Navigazionnaya Sputnikovaya Sistema, or Global Navigation Satellite System) is a global GNSS owned and operated by the Russian Federation. The fully operational system consists of 24+ satellites.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.gps.gov/systems/gnss/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Global Navigation Satellite System</rdfs:label>
        <rdfs:label xml:lang="ru">Globalnaya Navigazionnaya Sputnikovaya Sistema</rdfs:label>
        <ssn:hasDeployment rdf:resource="http://purl.org/poso/common/EarthDeployment"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/poso/common/GPS -->


    <owl:NamedIndividual rdf:about="http://purl.org/poso/common/GPS">
        <rdf:type rdf:resource="http://purl.org/poso/SatellitePositioningSystem"/>
        <rdfs:comment xml:lang="en">The Global Positioning System (GPS) is a U.S.-owned utility that provides users with positioning, navigation, and timing (PNT) services. This system consists of three segments: the space segment, the control segment, and the user segment. The U.S. Space Force develops, maintains, and operates the space and control segments.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">https://www.gps.gov/systems/gps/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Global Positioning System</rdfs:label>
        <ssn:hasDeployment rdf:resource="http://purl.org/poso/common/EarthDeployment"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/poso/common/Galileo -->


    <owl:NamedIndividual rdf:about="http://purl.org/poso/common/Galileo">
        <rdf:type rdf:resource="http://purl.org/poso/SatellitePositioningSystem"/>
        <rdfs:comment xml:lang="en">Galileo is a global GNSS owned and operated by the European Union. The EU declared the start of Galileo Initial Services in 2016 and plans to complete the system of 24+ satellites in 2021.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.gps.gov/systems/gnss/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Galileo</rdfs:label>
        <ssn:hasDeployment rdf:resource="http://purl.org/poso/common/EarthDeployment"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/poso/common/GeolocationAPI -->


    <owl:NamedIndividual rdf:about="http://purl.org/poso/common/GeolocationAPI">
        <rdf:type rdf:resource="http://purl.org/poso/LocationBasedService"/>
        <rdfs:comment xml:lang="en">The Geolocation API is a W3C specification that provides geographical location data based on the capabilities of the hosting device.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.w3.org/TR/geolocation/</rdfs:isDefinedBy>
        <rdfs:label>Geolocation API</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/poso/common/HectorSLAM -->


    <owl:NamedIndividual rdf:about="http://purl.org/poso/common/HectorSLAM">
        <rdf:type rdf:resource="http://purl.org/poso/VSLAM"/>
        <rdfs:label xml:lang="en">Hector SLAM</rdfs:label>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://wiki.ros.org/hector_slam</rdfs:seeAlso>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/poso/common/IRNSS -->


    <owl:NamedIndividual rdf:about="http://purl.org/poso/common/IRNSS">
        <rdf:type rdf:resource="http://purl.org/poso/SatellitePositioningSystem"/>
        <owl:sameAs rdf:resource="http://purl.org/poso/common/NavIC"/>
        <rdfs:comment xml:lang="en">IRNSS is a regional GNSS owned and operated by the Government of India. IRNSS is an autonomous system designed to cover the Indian region and 1500 km around the Indian mainland. The system consists of 7 satellites. In 2016, India renamed IRNSS as the Navigation Indian Constellation (NavIC, meaning &quot;sailor&quot; or &quot;navigator&quot;).</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.gps.gov/systems/gnss/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Indian Regional Navigation Satellite System</rdfs:label>
        <ssn:hasDeployment rdf:resource="http://purl.org/poso/common/EarthDeployment"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/poso/common/NavIC -->


    <rdf:Description rdf:about="http://purl.org/poso/common/NavIC"/>
    


    <!-- http://purl.org/poso/common/IndoorAtlas -->


    <owl:NamedIndividual rdf:about="http://purl.org/poso/common/IndoorAtlas">
        <rdf:type rdf:resource="http://purl.org/poso/PositioningPlatform"/>
        <rdfs:comment xml:lang="en">Cross-platform indoor positioning SDK powered by patented 6-layer sensor fusion core. Fast development cycles. Easy to set up. Proven by 10,000+ developers.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.indooratlas.com/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">IndoorAtlas</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/poso/common/KNNFingerprinting -->


    <owl:NamedIndividual rdf:about="http://purl.org/poso/common/KNNFingerprinting">
        <rdf:type rdf:resource="http://purl.org/poso/Fingerprinting"/>
        <rdfs:comment xml:lang="en">k-Neirest Neighbour fingerprinting is a fingerprinting technique where a number (k) of matches are selected, as opposed to one single fingerprint match. The final output position is the (weighted) average of the k-number of selected fingerprints.</rdfs:comment>
        <rdfs:label xml:lang="en">k-NN fingerprinting</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/poso/common/LDPL -->


    <owl:NamedIndividual rdf:about="http://purl.org/poso/common/LDPL">
        <rdf:type rdf:resource="http://purl.org/poso/RadioPropagation"/>
        <rdfs:comment xml:lang="en">The log-distance path loss model is a radio propagation model that predicts the path loss a signal encounters inside a building or densely populated areas over distance.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dbpedia.org/resource/Log-distance_path_loss_model"/>
        <rdfs:label xml:lang="en">Log-distance path loss model</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/poso/common/MidpointLateration -->


