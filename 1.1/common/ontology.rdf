    <!-- http://purl.org/poso/common/proximityUUID -->


    <owl:DatatypeProperty rdf:about="http://purl.org/poso/common/proximityUUID">
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/poso/common/proximityUUID"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#hexBinary"/>
            </owl:Restriction>
        </rdfs:domain>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#hexBinary"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:maxLength rdf:datatype="http://www.w3.org/2001/XMLSchema#int">32</xsd:maxLength>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:label xml:lang="en">proximity UUID</rdfs:label>
    </owl:DatatypeProperty>
    


