    </owl:DatatypeProperty>
    


    <!-- http://purl.org/poso/common/minor -->


    <owl:DatatypeProperty rdf:about="http://purl.org/poso/common/minor">
        <rdfs:domain rdf:resource="http://purl.org/poso/common/iBeacon"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/poso/common/minor"/>
