    <!-- http://purl.org/poso/common/AltBeacon -->


    <owl:Class rdf:about="http://purl.org/poso/common/AltBeacon">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso/BluetoothBeacon"/>
        <rdfs:comment xml:lang="en">AltBeacon is an open and interoperable proximity beacon specification.</rdfs:comment>
        <rdfs:label xml:lang="en">AltBeacon</rdfs:label>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://altbeacon.org/</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/poso/common/EddystoneBeacon -->


    <owl:Class rdf:about="http://purl.org/poso/common/EddystoneBeacon">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso/BluetoothBeacon"/>
        <rdfs:comment xml:lang="en">Eddystone was a Bluetooth Low Energy beacon profile released by Google in July 2015.</rdfs:comment>
        <rdfs:label xml:lang="en">Eddystone</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://dbpedia.org/resource/Eddystone_(Google)"/>
    </owl:Class>
    


    <!-- http://purl.org/poso/common/EddystoneEID -->


    <owl:Class rdf:about="http://purl.org/poso/common/EddystoneEID">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso/common/EddystoneBeacon"/>
        <rdfs:label xml:lang="en">Eddystone-EID</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso/common/EddystoneTLM -->


    <owl:Class rdf:about="http://purl.org/poso/common/EddystoneTLM">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso/common/EddystoneBeacon"/>
        <rdfs:comment xml:lang="en">Eddystone-TLM provides telemetry information. It can include the battery voltage, temperature, advertisement count and uptime.</rdfs:comment>
        <rdfs:label xml:lang="en">Eddystone-TLM</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso/common/EddystoneUID -->


    <owl:Class rdf:about="http://purl.org/poso/common/EddystoneUID">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso/common/EddystoneBeacon"/>
        <rdfs:comment xml:lang="en">Eddystone UID provides a 10 byte namespace and 6 byte instance identifier.</rdfs:comment>
        <rdfs:label xml:lang="en">Eddystone-UID</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso/common/EddystoneURL -->


    <owl:Class rdf:about="http://purl.org/poso/common/EddystoneURL">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso/common/EddystoneBeacon"/>
        <rdfs:comment xml:lang="en">Eddystone-URL can broadcast a short encoded URL.</rdfs:comment>
        <rdfs:label xml:lang="en">Eddystone-URL</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/poso/common/SemBeacon -->


    <owl:Class rdf:about="http://purl.org/poso/common/SemBeacon">
        <rdfs:subClassOf rdf:resource="http://purl.org/poso/BluetoothBeacon"/>
        <rdfs:comment xml:lang="en">A SemBeacon is a semantic beacon based on AltBeacon and Eddystone-URL.</rdfs:comment>
        <rdfs:label xml:lang="en">SemBeacon</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.org/sembeacon/SemBeacon"/>
    </owl:Class>
    


