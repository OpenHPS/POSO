    <!-- http://purl.org/poso/common/major -->


    <owl:DatatypeProperty rdf:about="http://purl.org/poso/common/major">
        <rdfs:domain rdf:resource="http://purl.org/poso/common/iBeacon"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/poso/common/major"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
