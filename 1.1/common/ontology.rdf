    <owl:Ontology rdf:about="http://purl.org/poso/common/">
        <owl:versionIRI rdf:resource="http://purl.org/poso/1.1/common/"/>
        <terms:abstract xml:lang="en">The common allignment module is a module that extends POSO with implementations of specific positioning techniques and algorithms - as well as known deployed systems and available platforms.</terms:abstract>
        <terms:bibliographicCitation>Maxim Van de Wynckel and Beat Signer, POSO: A Generic Positioning System Ontology</terms:bibliographicCitation>
        <terms:contributor xml:lang="en">Beat Signer</terms:contributor>
        <terms:creator xml:lang="en">Maxim Van de Wynckel</terms:creator>
        <terms:description>This ontology provides the terminologies used for positioning systems.</terms:description>
        <terms:license rdf:resource="http://purl.org/NET/rdflicense/MIT1.0"/>
        <terms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-12-16T00:00:00</terms:modified>
        <terms:title xml:lang="en">Positioning System Ontology Common Technologies, Algorithms and Systems</terms:title>
        <vann:preferredNamespacePrefix>posoc</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/poso/common/</vann:preferredNamespaceUri>
    </owl:Ontology>
    


